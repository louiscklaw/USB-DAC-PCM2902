
(rules PCB USB-DAC-PCM2902
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 544)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.8)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 400.0)
    (clear 200.2)
    (clear 200.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_2500:1000_um"
    (shape
      (circle F.Cu 2500.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 2500.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_2500:1000_um" "Via[0-1]_2500:1000_um" default
  )
  (via 
    "Via[0-1]_2500:1000_um-kicad_default" "Via[0-1]_2500:1000_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_2500:1000_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_2500:1000_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(C3-Pad2)" "Net-(C5-Pad2)" "Net-(R3-Pad2)" "Net-(R1-Pad2)" GND "Net-(R4-Pad2)" "Net-(C7-Pad2)" "Net-(C7-Pad1)"
    "Net-(D1-Pad2)" "Net-(R3-Pad1)" "Net-(R2-Pad1)" "Net-(C1-Pad1)" "Net-(U3-Pad2)" "Net-(U3-Pad3)" "Net-(U3-Pad5)" "Net-(U3-Pad6)"
    "Net-(C10-Pad1)" "Net-(C11-Pad1)" "Net-(C8-Pad1)" "Net-(C6-Pad1)" "Net-(C4-Pad1)" "Net-(C2-Pad1)" "Net-(J1-Pad2)" "Net-(J2-Pad4)"
    /OUTR /OUTL "/VOL_MUTE" "/VOL_DOWN" "/VOL_UP" /PIN28
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)